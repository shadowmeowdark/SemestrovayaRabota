<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="440.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="WorkWithData.LoadDataController">
    <children>
        <Pane prefHeight="440.0" prefWidth="750.0" style="-fx-background-color: #333333;">
            <children>
                <Button fx:id="buttonChooseFile" layoutX="331.0" layoutY="55.0" mnemonicParsing="false"
                        prefHeight="28.0" prefWidth="85.0"
                        style="-fx-background-color: #ffffff00; -fx-background-radius: 5px; -fx-border-color: #ffffff; -fx-border-radius: 5px; -fx-border-width: 1px; -fx-text-fill: whitesmoke;"
                        text="Select" textFill="WHITE"/>
                <Label layoutX="267.0" layoutY="26.0" text="Select file from computer" textFill="WHITE">
                    <font>
                        <Font size="17.0"/>
                    </font>
                </Label>
                <Label layoutX="284.0" layoutY="127.0" text="Link for Downloading" textFill="WHITE">
                    <font>
                        <Font size="17.0"/>
                    </font>
                </Label>
                <TextField fx:id="linkForDownloading" layoutX="236.0" layoutY="156.0" prefHeight="26.0"
                           prefWidth="278.0" promptText="URL"
                           style="-fx-background-color: #ffffff00; -fx-background-radius: 5px; -fx-border-color: #ffffff; -fx-border-radius: 5px; -fx-border-width: 1px; -fx-text-fill: whitesmoke;"
                />
                <Button fx:id="buttonDownload" layoutX="333.0" layoutY="192.0" mnemonicParsing="false" prefHeight="28.0"
                        prefWidth="85.0"
                        style="-fx-background-color: #ffffff00; -fx-background-radius: 5px; -fx-border-color: #ffffff; -fx-border-radius: 5px; -fx-border-width: 1px; -fx-text-fill: whitesmoke;"
                        text="Download" textFill="WHITE"/>
                <Button fx:id="buttonGraphics" layoutX="151.0" layoutY="336.0" mnemonicParsing="false" prefHeight="28.0"
                        prefWidth="85.0"
                        style="-fx-background-color: #ffffff00; -fx-background-radius: 5px; -fx-border-color: #ffffff; -fx-border-radius: 5px; -fx-border-width: 1px; -fx-text-fill: whitesmoke;"
                        text="Grapchis" textFill="WHITE"/>
                <Button fx:id="logout" layoutX="514.0" layoutY="336.0" mnemonicParsing="false" prefHeight="28.0"
                        prefWidth="85.0"
                        style="-fx-background-color: #ffffff00; -fx-background-radius: 5px; -fx-border-color: #ffffff; -fx-border-radius: 5px; -fx-border-width: 1px; -fx-text-fill: whitesmoke;"
                        text="Log Out" textFill="WHITE"/>
                <Label layoutX="366.0" layoutY="96.0" text="or" textFill="WHITE">
                    <font>
                        <Font size="17.0"/>
                    </font>
                </Label>
            </children>
        </Pane>
    </children>
</AnchorPane>
